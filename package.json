{
  "name": "foodmission-data-framework",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "license": "AGPL-3.0-or-later",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:unit": "jest --testPathPatterns=\"src/.*\\.spec\\.ts$\"",
    "test:integration": "jest --testPathPatterns=\"test/.*\\.integration\\.spec\\.ts$\"",
    "test:e2e": "jest --config test/jest-e2e.json",
    "test:all": "jest --coverage --testPathPatterns=\"(src/.*\\.spec\\.ts$|test/.*\\.(integration|e2e)-spec\\.ts$)\"",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:migrate:reset": "prisma migrate reset",
    "db:studio": "prisma studio",
    "db:seed": "prisma db seed",
    "db:seed:dev": "ts-node scripts/seed-dev.ts",
    "db:seed:test": "ts-node scripts/seed-test.ts",
    "db:seed:foods": "ts-node scripts/seed-foods-only.ts",
    "db:backup": "./scripts/db-backup.sh",
    "db:backup:full": "./scripts/db-backup.sh full",
    "db:backup:data": "./scripts/db-backup.sh data",
    "db:backup:schema": "./scripts/db-backup.sh schema",
    "db:backup:all": "./scripts/db-backup.sh all",
    "db:restore": "./scripts/db-restore.sh",
    "db:restore:latest": "./scripts/db-restore.sh latest",
    "db:restore:interactive": "./scripts/db-restore.sh interactive",
    "migrate": "ts-node scripts/migration-utils.ts",
    "dev:setup": "./scripts/dev-setup.sh",
    "dev:reset": "./scripts/db-reset.sh",
    "ci:test": "./scripts/ci-local-test.sh",
    "docker:up": "docker compose -f docker-compose.dev.yml up -d",
    "docker:down": "docker compose -f docker-compose.dev.yml down",
    "docker:logs": "docker compose -f docker-compose.dev.yml logs -f",
    "docs:generate": "ts-node scripts/generate-openapi-spec.ts",
    "docs:generate:ci": "./scripts/ci-docs-generate.sh",
    "openapi:generate": "node scripts/generate-openapi.js",
    "openapi:serve": "npm run build && npm run openapi:generate && echo 'OpenAPI spec generated! Import docs/openapi.json into Postman'",
    "postman:collection": "npm run openapi:serve"
  },
  "dependencies": {
    "@keyv/redis": "^5.1.2",
    "@nestjs/axios": "^4.0.1",
    "@nestjs/cache-manager": "3.0.1",
    "@nestjs/common": "^11.1.6",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.6",
    "@nestjs/platform-express": "^11.1.6",
    "@nestjs/swagger": "^11.2.0",
    "@nestjs/terminus": "^11.0.0",
    "@nestjs/throttler": "^6.4.0",
    "@prisma/client": "^6.16.2",
    "axios": "^1.12.2",
    "cache-manager": "^7.2.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "cls-hooked": "^4.2.2",
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "helmet": "^8.1.0",
    "joi": "^18.0.1",
    "keycloak-connect": "^26.1.1",
    "keyv": "^5.5.1",
    "nest-keycloak-connect": "^2.0.0-alpha.2",
    "nest-winston": "^1.10.2",
    "prisma": "^6.16.2",
    "prom-client": "^15.1.3",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "sanitize-html": "^2.17.0",
    "uuid": "^13.0.0",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.35.0",
    "@nestjs/cli": "^11.0.10",
    "@nestjs/schematics": "^11.0.7",
    "@nestjs/testing": "^11.1.6",
    "@types/cache-manager": "^5.0.0",
    "@types/express": "^5.0.3",
    "@types/helmet": "^4.0.0",
    "@types/jest": "^30.0.0",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^24.5.0",
    "@types/pg": "^8.15.5",
    "@types/sanitize-html": "^2.16.0",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^11.0.0",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "globals": "^16.4.0",
    "jest": "^30.1.3",
    "js-yaml": "^4.1.0",
    "prettier": "^3.6.2",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.2",
    "ts-loader": "^9.5.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.44.0"
  }
}
